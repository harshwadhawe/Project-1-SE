# === Rails project (pc_builder) ===

# Bundler config
/pc_builder/.bundle

# Environment files
/pc_builder/.env*

# Logs and tempfiles
/pc_builder/log/*
/pc_builder/tmp/*
!/pc_builder/log/.keep
!/pc_builder/tmp/.keep
!/pc_builder/tmp/.kamal
# PIDs
/pc_builder/tmp/pids/*
!/pc_builder/tmp/pids/
/pc_builder/tmp/pids/.keep

# Storage (uploaded files, SQLite, etc.)
/pc_builder/storage/*
!/pc_builder/storage/.keep
/pc_builder/tmp/storage/*
!/pc_builder/tmp/storage/
/pc_builder/tmp/storage/.keep

# Precompiled assets
/pc_builder/public/assets

# Rails master key
/pc_builder/config/master.key

# === General OS/editor cruft ===
.DS_Store
Thumbs.db
*.swp
*.swo
*.bak

/pc_builder/db/*.sqlite3
/pc_builder/db/*.sqlite3-journal
/pc_builder/log/*
!/pc_builder/log/.keep
/pc_builder/tmp/*
!/pc_builder/tmp/.keep
/pc_builder/storage/*
!/pc_builder/storage/.keep
/pc_builder/public/assets
/pc_builder/config/master.key

